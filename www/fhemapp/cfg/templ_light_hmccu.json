{  "name": "light_hmccu",  "author": "guwo",  "date": "2021-03-21",  "status": {    "bar": ["state:on:100:success","state:off:0:success"],    "error": ["activity.Value:^(?!alive):100:error:keine Verbindung"]  },  "main": [    {      "leftBtn": "mdi-power-off",      "leftClick": ["state::off"],      "text": ["state:on:an","state:off:aus","state::%s"],      "rightBtn": "mdi-power-on",      "rightClick": ["state::on"]    }  ],  "info": {    "left1": ["state:on::mdi-lightbulb","state:off::mdi-lightbulb-off"],    "right1": ["battery:ok::mdi-battery","battery:::mdi-battery-10"],    "right2": ["activity:alive::mdi-wifi","activity:::mdi-wifi-off"]  }}